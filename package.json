  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "default": "./dist/index.es.js"
    }
  },
  "author": {
    "name": "Quark",
    "email": "caodahong@gmail.com"
  }
